# ═══════════════════════════════════════════════════════════════════════════════
#                    🏛️ AUTUS EMPIRE v4.0.0 - Environment Variables
# ═══════════════════════════════════════════════════════════════════════════════
#
# 사용법:
#   1. 이 파일을 .env로 복사: cp env.empire.example .env
#   2. 값들을 실제 환경에 맞게 수정
#   3. 서버 실행: uvicorn backend.main_final:app
#
# ═══════════════════════════════════════════════════════════════════════════════

# ───────────────────────────────────────────────────────────────────────────────
# 서버 설정
# ───────────────────────────────────────────────────────────────────────────────

# 환경 (development, staging, production)
ENV=development

# 서버 주소
HOST=0.0.0.0
PORT=8000

# ───────────────────────────────────────────────────────────────────────────────
# 인증 설정
# ───────────────────────────────────────────────────────────────────────────────

# 인증 활성화 (true/false)
AUTH_ENABLED=false

# JWT 시크릿 키 (프로덕션에서 반드시 변경!)
JWT_SECRET_KEY=autus-empire-secret-key-change-in-production

# 마스터 API 키 (관리자용)
MASTER_API_KEY=

# ───────────────────────────────────────────────────────────────────────────────
# 데이터베이스 설정
# ───────────────────────────────────────────────────────────────────────────────

# SQLite (기본값)
DATABASE_URL=sqlite:///./autus_empire.db

# PostgreSQL 예시
# DATABASE_URL=postgresql://user:password@localhost:5432/autus_empire

# SQL 로깅 (true/false)
ECHO_SQL=false

# 커넥션 풀 (PostgreSQL용)
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10

# ───────────────────────────────────────────────────────────────────────────────
# Redis 설정 (선택)
# ───────────────────────────────────────────────────────────────────────────────

# Redis 사용 여부
USE_REDIS=false

# Redis URL
REDIS_URL=redis://localhost:6379/0

# ───────────────────────────────────────────────────────────────────────────────
# Rate Limiting 설정
# ───────────────────────────────────────────────────────────────────────────────

# Rate Limiting 활성화
RATE_LIMIT_ENABLED=true

# 윈도우당 최대 요청 수
RATE_LIMIT_REQUESTS=100

# 시간 윈도우 (초)
RATE_LIMIT_WINDOW=60

# ───────────────────────────────────────────────────────────────────────────────
# 캐싱 설정
# ───────────────────────────────────────────────────────────────────────────────

# 캐시 활성화
CACHE_ENABLED=true

# 기본 TTL (초)
CACHE_DEFAULT_TTL=300

# ───────────────────────────────────────────────────────────────────────────────
# 로깅 설정
# ───────────────────────────────────────────────────────────────────────────────

# 로그 레벨 (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# 로그 파일 경로
LOG_FILE=logs/autus.log

# JSON 로그 출력 (프로덕션용)
JSON_LOGS=false

# ───────────────────────────────────────────────────────────────────────────────
# 외부 API 연동 (선택)
# ───────────────────────────────────────────────────────────────────────────────

# 기상청 API (공공데이터포털)
WEATHER_API_KEY=

# SMS - 알리고
ALIGO_API_KEY=
ALIGO_USER_ID=
ALIGO_SENDER=

# SMS - NHN Cloud
NHN_APP_KEY=
NHN_SECRET_KEY=
NHN_SENDER=

# 카카오 알림톡
KAKAO_API_KEY=
KAKAO_SENDER_KEY=

# Slack 웹훅
SLACK_WEBHOOK_URL=

# Discord 웹훅
DISCORD_WEBHOOK_URL=

# ───────────────────────────────────────────────────────────────────────────────
# 이메일 (SMTP)
# ───────────────────────────────────────────────────────────────────────────────

SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=

# ───────────────────────────────────────────────────────────────────────────────
# 메트릭스 & 모니터링 (선택)
# ───────────────────────────────────────────────────────────────────────────────

# Prometheus 메트릭스 활성화
METRICS_ENABLED=false

# Sentry DSN (에러 추적)
SENTRY_DSN=

# ═══════════════════════════════════════════════════════════════════════════════
# 끝
# ═══════════════════════════════════════════════════════════════════════════════






