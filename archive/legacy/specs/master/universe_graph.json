{
  "name": "AUTUS Universe Graph",
  "version": "1.0.0",
  "description": "Universe 그래프 구조 - 1-2-3-4-Universe 모델",
  
  "ontology": {
    "entity_types": {
      "User": {
        "description": "사용자 (Zero Identity)",
        "layer": "1_identity",
        "properties": {
          "zero_id": { "type": "string", "required": true },
          "coordinates_3d": { "type": "object", "required": true },
          "auth_type": { "type": "string", "default": "zero_auth" },
          "created_at": { "type": "datetime", "required": true }
        }
      },
      "City": {
        "description": "도시/세계 (Twin World)",
        "layer": "3_worlds",
        "properties": {
          "city_id": { "type": "string", "required": true },
          "name": { "type": "string", "required": true },
          "population": { "type": "integer" },
          "status": { "type": "string", "enum": ["active", "inactive", "planned"] },
          "coordinates": { "type": "object" }
        }
      },
      "Pack": {
        "description": "실행 팩 (Pack Engine)",
        "layer": "4_packs",
        "properties": {
          "pack_id": { "type": "string", "required": true },
          "name": { "type": "string", "required": true },
          "version": { "type": "string" },
          "status": { "type": "string", "enum": ["active", "beta", "planned", "deprecated"] },
          "goal": { "type": "string" }
        }
      },
      "Workflow": {
        "description": "워크플로우",
        "layer": "4_packs",
        "properties": {
          "workflow_id": { "type": "string", "required": true },
          "name": { "type": "string", "required": true },
          "steps": { "type": "array" },
          "status": { "type": "string" }
        }
      },
      "Event": {
        "description": "이벤트 (Reality Event)",
        "layer": "3_worlds",
        "properties": {
          "event_id": { "type": "string", "required": true },
          "type": { "type": "string", "required": true },
          "timestamp": { "type": "datetime", "required": true },
          "payload": { "type": "object" },
          "source": { "type": "string" }
        }
      },
      "Memory": {
        "description": "메모리 (Sovereign)",
        "layer": "2_sovereign",
        "properties": {
          "memory_id": { "type": "string", "required": true },
          "type": { "type": "string", "enum": ["preference", "pattern", "workflow"] },
          "data": { "type": "object" },
          "updated_at": { "type": "datetime" }
        }
      }
    },
    
    "edge_types": {
      "influence": {
        "description": "영향을 미침",
        "from": ["User", "Pack"],
        "to": ["City", "Workflow", "Pack"],
        "properties": {
          "weight": { "type": "float", "min": 0, "max": 1 },
          "type": { "type": "string" }
        }
      },
      "affects": {
        "description": "영향을 받음",
        "from": ["Event"],
        "to": ["User", "City", "Pack", "Workflow"],
        "properties": {
          "impact_score": { "type": "float" }
        }
      },
      "depends_on": {
        "description": "의존함",
        "from": ["Pack", "Workflow"],
        "to": ["Pack", "Workflow"],
        "properties": {
          "required": { "type": "boolean" }
        }
      },
      "produces": {
        "description": "생성함",
        "from": ["Pack", "Workflow"],
        "to": ["Event"],
        "properties": {
          "frequency": { "type": "string" }
        }
      },
      "belongs_to": {
        "description": "소속됨",
        "from": ["User"],
        "to": ["City"],
        "properties": {
          "role": { "type": "string" },
          "since": { "type": "datetime" }
        }
      },
      "uses": {
        "description": "사용함",
        "from": ["User", "City"],
        "to": ["Pack"],
        "properties": {
          "frequency": { "type": "string" },
          "last_used": { "type": "datetime" }
        }
      },
      "stores": {
        "description": "저장함",
        "from": ["User"],
        "to": ["Memory"],
        "properties": {
          "consent": { "type": "boolean" }
        }
      }
    }
  },
  
  "sample_graph": {
    "nodes": [
      { "id": "user_001", "type": "User", "zero_id": "ZzdYfGcLhSmeSBu9cH4J5TA", "coordinates_3d": {"x": 0.5, "y": -0.3, "z": 0.8} },
      { "id": "seoul", "type": "City", "name": "Seoul", "population": 10000000, "status": "active" },
      { "id": "clark", "type": "City", "name": "Clark", "population": 500000, "status": "active" },
      { "id": "kathmandu", "type": "City", "name": "Kathmandu", "population": 1500000, "status": "active" },
      { "id": "school_pack", "type": "Pack", "name": "School", "status": "active", "goal": "Operate education systems" },
      { "id": "visa_pack", "type": "Pack", "name": "Visa", "status": "active", "goal": "Manage talent migration" },
      { "id": "cmms_pack", "type": "Pack", "name": "CMMS", "status": "active", "goal": "Maintain facilities" },
      { "id": "admissions_pack", "type": "Pack", "name": "Admissions", "status": "active", "goal": "Process applications" }
    ],
    "edges": [
      { "from": "user_001", "to": "seoul", "type": "belongs_to", "role": "resident" },
      { "from": "user_001", "to": "school_pack", "type": "uses", "frequency": "daily" },
      { "from": "user_001", "to": "visa_pack", "type": "uses", "frequency": "monthly" },
      { "from": "seoul", "to": "clark", "type": "influence", "weight": 0.7, "relation": "talent_flow" },
      { "from": "clark", "to": "kathmandu", "type": "influence", "weight": 0.5, "relation": "education_partnership" },
      { "from": "kathmandu", "to": "seoul", "type": "influence", "weight": 0.4, "relation": "visa_program" },
      { "from": "school_pack", "to": "visa_pack", "type": "depends_on", "required": false },
      { "from": "admissions_pack", "to": "school_pack", "type": "depends_on", "required": true }
    ]
  },
  
  "pillars_in_graph": {
    "information": {
      "description": "노드와 노드 속성",
      "components": ["nodes", "node.properties"]
    },
    "context": {
      "description": "엣지와 관계",
      "components": ["edges", "edge.properties"]
    },
    "intent": {
      "description": "Pack goals와 User actions",
      "components": ["Pack.goal", "User.coordinates_3d"]
    },
    "impact": {
      "description": "influence weights와 metrics",
      "components": ["edge.weight", "edge.impact_score"]
    }
  },
  
  "metrics": {
    "total_nodes": 8,
    "total_edges": 8,
    "connectivity": 0.90,
    "health": 0.85,
    "by_type": {
      "User": 1,
      "City": 3,
      "Pack": 4
    }
  },
  
  "queries": {
    "examples": [
      {
        "name": "Get user's cities",
        "query": "MATCH (u:User)-[:belongs_to]->(c:City) WHERE u.zero_id = $zero_id RETURN c"
      },
      {
        "name": "Get pack dependencies",
        "query": "MATCH (p1:Pack)-[:depends_on]->(p2:Pack) WHERE p1.pack_id = $pack_id RETURN p2"
      },
      {
        "name": "Get influence paths",
        "query": "MATCH path = (c1:City)-[:influence*1..3]->(c2:City) RETURN path"
      }
    ]
  }
}
