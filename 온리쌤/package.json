{
  "name": "onlyssam",
  "version": "1.0.0",
  "description": "온리쌤 유니버설 학원 관리 시스템 - QR 출석, 역할별 대시보드, 학생 관리",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "ci": "npm run typecheck && npm run test -- --ci",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production --latest",
    "submit:android": "eas submit --platform android --profile production --latest",
    "update": "eas update --auto --channel production",
    "update:preview": "eas update --auto --channel preview",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:migrate": "supabase db push",
    "supabase:types": "supabase gen types typescript --local > src/types/supabase.ts"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-native-seoul/kakao-login": "^5.4.2",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/drawer": "^6.6.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/native-stack": "^6.9.0",
    "@sentry/react-native": "5.36.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.17.0",
    "axios": "^1.6.0",
    "date-fns": "^3.2.0",
    "expo": "~54.0.33",
    "expo-auth-session": "~7.0.10",
    "expo-av": "~16.0.8",
    "expo-camera": "~17.0.10",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-notifications": "~0.32.16",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "expo-video-thumbnails": "~10.0.8",
    "expo-web-browser": "~15.0.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-view-shot": "3.8.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "13.6.4",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/react": "~18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eas-cli": "^18.0.1",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^29.0.0",
    "jest-expo": "~50.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.3.0"
  },
  "private": true
}
