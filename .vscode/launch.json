{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "AUTUS Server",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": ["app.main:app", "--reload", "--port", "8000"],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "AUTUS Tests",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "args": ["-v", "tests/"]
        }
    ]
}
