{
  "deno.enable": true,
  "deno.enablePaths": [
    "./supabase/functions"
  ],
  "deno.disablePaths": [
    "./moltbot-brain",
    "./moltbot-bridge",
    "./kraton-v2"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "javascript.validate.enable": true,
  "typescript.validate.enable": true,
  "files.associations": {
    "*.ts": "typescript",
    "*.js": "javascript",
    "*.jsx": "javascriptreact",
    "*.tsx": "typescriptreact"
  },
  "editor.formatOnSave": false,
  "editor.tabSize": 2,
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true
  }
}
